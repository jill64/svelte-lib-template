{
  "name": "xxxxxxxxxxxxxxxxxxxx",
  "version": "0.0.0",
  "private": true,
  "description": "⛩️ Svelte Library Template",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx psvx",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-svelte": "2.0.5",
    "@jill64/npm-demo-layout": "2.0.15",
    "@jill64/universal-sanitizer": "1.4.1",
    "@jill64/playwright-config": "2.4.2",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "2.0.5",
    "@playwright/test": "1.50.0",
    "@sveltejs/adapter-cloudflare": "5.0.1",
    "@sveltejs/kit": "2.16.0",
    "svelte": "5.19.0",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vitest": "3.0.4",
    "@sveltejs/vite-plugin-svelte": "5.0.3"
  },
  "homepage": "https://github.com/jill64/svelte-lib-template#readme",
